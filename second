dimension_group: appointment_booked_london {
  group_label: "Data â€“ Dimensions-London_Time"
  label: "Appointment Booked (London)"
  type: time
  timeframes: [raw, time, date, week, month, quarter, year, hour_of_day, day_of_week, day_of_week_index]
  sql:
